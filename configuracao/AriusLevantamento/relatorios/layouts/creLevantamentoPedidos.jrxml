	<queryString>
		<![CDATA[select
pf.referencia ref,
pr.id,
substr(pr.descritivo,1,50) produto1,
substr(pr.descritivo,31,60)produto2,
pp.venda,
 ' ' as sugt,
 ' ' as sugtforn,
 (select round(custo,3) from proreg.vw_produtosa_precos pp where pp.id = pr.id and pp.tipo_venda = 1 and pp.estado = 'SP' and politica = 1 ) custo_tabela,
 (select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa in(2,3,5,1,99,6,4,11,9,8,15,16,17))Sg_total,

 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 1 )sg_gta1,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 2 )sg_crz,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 3 )sg_lor1,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 4 )sg_taub1,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 5 )sg_lor2,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 6 )sg_pinda,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 8 )sg_sjc,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 9 )sg_cacap,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 11 )sg_taub2,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 15 )sg_jac,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 16 )sg_sjcHumel,
 (select 'Sug'||'  '||nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 17 )sg_buriti,

 (select qtde_embalageme from proreg.vw_tabela_fornecedor where fornecedor = $P{pIdFornecedor} and produto = pr.id and estado = 'SP')qtde_embalagem,

 (select ped.data from   (select max(datahora_cadastro) data, custo, produto, qtde  from
 (  select p.id, pi.pedido, pi.produto, p.empresa, p.datahora_cadastro, pi.custo , pi.qtde_entrada qtde
   from proreg.pedidos_itens pi, proreg.pedidos p
   where  pi.pedido = p.id and p.fornecedor  = $P{pIdFornecedor} and p.empresa >= $P{pIdEmpresa}  order by p.datahora_cadastro desc)  group by custo, produto, qtde) ped
   where  ped.produto = pr.id and  rownum = 1 )data_ult_entrada_ped,

   (select ped.custo from
   (select max(datahora_cadastro) data, custo, produto, qtde  from
   ( select p.id, pi.pedido, pi.produto, p.empresa, p.datahora_cadastro, pi.custo , pi.qtde_entrada qtde
   from proreg.pedidos_itens pi, proreg.pedidos p
   where  pi.pedido = p.id and p.fornecedor  = $P{pIdFornecedor} and p.empresa >= $P{pIdEmpresa} and pi.datahora_entrada is not null order by p.datahora_cadastro desc) group by custo, produto, qtde) ped
   where ped.produto = pr.id and rownum = 1 )custo_ult_enrada_ped2,

   (select sum(qtde) from( select p.id, trunc(p.datahora_cadastro) data, sum(pi.qtde_entrada) qtde, p.empresa, pi.produto
   from proreg.pedidos_itens pi, proreg.pedidos p
   where pi.pedido = p.id and p.datahora_entrega is not null and  pi.datahora_entrada is not null and p.fornecedor = $P{pIdFornecedor}  and  trunc(p.datahora_cadastro) in
   (select max(trunc(datahora_cadastro)) data
   from proreg.pedidos where fornecedor = $P{pIdFornecedor} and trunc(datahora_cadastro) between  to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda})  )
   group by p.id, trunc(p.datahora_cadastro), p.empresa, pi.produto) where produto = pr.id )  qtdeCompra_ult_entrada_ped,

   (select SUM(qtde) from proreg.vw_ultima_compra where forn =  $P{pIdFornecedor}  and produto = pr.id and empresa >= $P{pIdEmpresa}   and entrada
   BETWEEN  to_date($P{pDataIncialCompra}) and to_date($P{pDataFinalCompra}) )ultcompra_x,

   (select entrada from proreg.vw_ultima_compra where forn =  $P{pIdFornecedor} and produto = pr.id and empresa in(99,1,16) and entrada in
   (select max(entrada) from proreg.vw_ultima_compra  where forn =  $P{pIdFornecedor} and produto = pr.id and empresa in(99,1,16)) and rownum = 1)dataultcompra,

   (select round(custo_anterior,3) as custo from proreg.tabela_fornecedor_uf where fornecedor = $P{pIdFornecedor} and produto = pr.id and rownum = 1) custo,
   (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque not in (7,10,14,199))total,

   (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda})  and produto = pr.id and empresa in(2,3,5,1,99,6,4,11,9,8,15,16,17))vdtotal,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 2)crz,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 2)vdcrz,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 3)lor1,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 3)vdlor1,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 5)lor2,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 5)vdlor2,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 1)gta1,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 1)vdgta1,
   (select  nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 99)deposito,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 99)vddeposito,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 6)pinda,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 6)vdpinda,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 4)taub,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 4)vdtaub,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 11)taub2,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 11)vdtaub2,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 9)caçp,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 9)vdcaçp,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 8)sjc,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 8)vdsjc,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 15)jac,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 15)vdjac,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 16)sjchumel,
   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 16)vdsjchumel,
   (select 'Est'||'  '||nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 17)buriti,

   (select 'Vnd'||'  '||nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 17)vdburiti

     from proreg.produtos_fornecedor pf,proreg.produtos pr,proreg.produtos_precos pp,proreg.produtos_loja pl where pr.id in
     (select vd.produto from (select * from proreg.vendas_diarias
     union all
     select * from proreg.vendas_diariasm  ) vd where vd.empresa >= $P{pIdEmpresa}  and  trunc(vd.data) between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda}) and  vd.tipovenda = 1 )
     and pf.produto = pr.id  and pp.produto = pr.id  and pp.id = 1 and pl.id = pf.produto and pp.politica =  $P{pIdEmpresa}
     and pl.politica =   $P{pIdEmpresa} and pf.fornecedor = $P{pIdFornecedor} and pr.status <> 3 order by produto1]]>
	</queryString>
