	<queryString>
		<![CDATA[select
pr.id,
pf.referencia ref,
substr(pr.descritivo,1,50) produto1,
substr(pr.descritivo,31,60) produto2, pp.venda,' ' as sugt,' ' as sugtforn,
(select round(custo,3) from proreg.vw_produtosa_precos pp where pp.id = pr.id and pp.tipo_venda = 1 and pp.estado = 'SP' and politica = 1 ) custo_tabela,

(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa in(2,3,5,1,99,6,4,11,9,8,15,16,17))Sg_total,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 1 )sug_guara,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 2 )sug_cruzeiro,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 3 )sug_lorena1,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 4 )sug_taubate1,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 5 )sug_lorena2,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 6 )sug_pinda,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 8 )sug_saoJose,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 9 )sug_cacapava,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 11 )sug_taubate2,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 15 )sug_jacarei,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 16 )sug_sjchumel,
(select nvl(sum(scompra),0) from proreg.vw_sugestao_compra where fornecedor = $P{pIdFornecedor} and produto = pr.id and empresa = 17 )sug_buriti,

(select ped.data from (select max(datahora_cadastro) data, custo, produto, qtde
  from(select p.id, pi.pedido, pi.produto, p.empresa, p.datahora_cadastro, pi.custo , pi.qtde_entrada qtde
  from proreg.pedidos_itens pi, proreg.pedidos p
  where  pi.pedido = p.id and p.fornecedor  = $P{pIdFornecedor} and p.empresa >= $P{pIdEmpresa} order by p.datahora_cadastro desc)  group by custo, produto, qtde) ped
    where ped.produto = pr.id and  rownum = 1 )data_ult_entrada_ped,

    (select ped.custo from   (select max(datahora_cadastro) data, custo, produto, qtde  from
    (  select p.id, pi.pedido, pi.produto, p.empresa, p.datahora_cadastro, pi.custo , pi.qtde_entrada qtde
    from proreg.pedidos_itens pi, proreg.pedidos p where  pi.pedido = p.id and p.fornecedor  = $P{pIdFornecedor} and p.empresa = $P{pIdEmpresa} order by p.datahora_cadastro desc)
     group by custo, produto, qtde) ped  where  ped.produto = pr.id and  rownum = 1 )custo_ult_enrada_ped2,

(select sum(qtde)
from
( select p.id, trunc(p.datahora_cadastro) data, sum(pi.qtde_entrada) qtde, p.empresa, pi.produto
     from proreg.pedidos_itens pi, proreg.pedidos p where pi.pedido = p.id and p.fornecedor = $P{pIdFornecedor}  and p.datahora_entrega is not null and trunc(p.datahora_cadastro) in
     (select max(trunc(datahora_cadastro)) data
       from proreg.pedidos where  fornecedor = $P{pIdFornecedor}  and trunc(p.datahora_cadastro) between to_date( $P{pdataIniVenda} ) and to_date( $P{pdataFinVenda} )  )
        and pi.datahora_entrada is not null group by p.id, trunc(p.datahora_cadastro), p.empresa, pi.produto)
        where produto = pr.id ) qtdeCompra_ult_entrada_ped,

          (select SUM(qtde) from proreg.vw_ultima_compra where forn = $P{pIdFornecedor}  and produto = pr.id and empresa >= $P{pIdEmpresa}
           and entrada BETWEEN to_date($P{pDataIncialCompra}) and to_date($P{pDataFinalCompra}) )ultcompra_x,

         (select entrada from proreg.vw_ultima_compra where forn = $P{pIdFornecedor} and produto = pr.id and empresa in(99,1,16) and entrada in(select max(entrada)
         from proreg.vw_ultima_compra where forn = $P{pIdFornecedor} and produto = pr.id and empresa in(99,1,16)) and rownum = 1)dataultcompra,

       (select round(custo_anterior,3) as custo from proreg.tabela_fornecedor_uf where fornecedor = $P{pIdFornecedor} and produto = pr.id and rownum = 1) custo,

      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque not in (7,10,14,199))total,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data)
      between to_date($P{pdataIniVenda})  and to_date($P{pdataFinVenda})  and produto = pr.id and empresa in(2,3,5,1,99,6,4,11,9,8,15,16,17))vdtotal,

      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 2)cruzeiroEstoque,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}  ) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 2)vendaCruzeiro,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 3)lorena1,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 3)vendalorena1,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 5)lorena2,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 5)vendalorena2,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 1)guara1Estoque,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 1)vendaguara1,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 99)deposito,

      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 99)vddeposito,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 6)pinda,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 6)vendapinda,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 4)taubate,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 4)vendataubate,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 11)taubate2,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 11)vendataubate2,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 9)caçapava,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 9)vendacaçapava,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 8)saojose,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 8)vendasaojose,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 15)jacarei,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 15)vendajacarei,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 16)sjchumel,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 16)vdsjchumel,
      (select nvl(sum(estoque_atual),0) from proreg.produtos_estoques where produto = pr.id and estoque = 17)buriti,
      (select nvl(sum(qtde),0) from proreg.vendas_diarias where trunc(data) between to_date($P{pdataIniVenda}) and to_date($P{pdataFinVenda}) and produto = pr.id and empresa = 17)vendaburiti
      from proreg.vw_produtos_fornecedor pf, proreg.produtos pr, proreg.produtos_precos pp, proreg.produtos_loja pl where pf.produto = pr.id and pp.produto = pr.id
      and pp.id = 1 and pl.id = pf.produto and pp.politica   = $P{pIdEmpresa} and pl.politica   = $P{pIdEmpresa} and pf.fornecedor = $P{pIdFornecedor} and pr.status <> 3

      order by produto1]]>
	</queryString>
